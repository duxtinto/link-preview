{
    "name": "duxtinto/link-preview",
    "type": "library",
    "description": "Link preview generation for PHP with Laravel support",
    "keywords": ["php","url","scraping","laravel","preview"],
    "homepage": "http://github.com/duxtinto/link-preview",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.*",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/dom-crawler": "^4.1.0",
        "symfony/css-selector": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Duxtinto\\LinkPreview\\": "src/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Duxtinto\\LinkPreview\\Integrations\\LaravelServiceProvider"
            ]
        }
    }
}
